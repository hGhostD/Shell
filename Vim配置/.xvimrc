set number 
set relativenumber
set hlsearch
set autoindent
set smartindent
set tabstop=4
set softtabstop=4
set expandtab
filetype on
syntax on                   "语法高亮"
set nocompatible            "兼容模式"
set cursorline              "显示当前编辑行"
set t_CO=256                "颜色数量"
set noerrorbells            "取消错误提示音"
set scrolljump=5            "自动滚动"
set scrolloff=3
:map - ddp
:map <space> viw
:inoremap jk <esc>          "在插入模式按jk退出插入模式"
:set ic                     "搜索忽略大小写"
